<Application x:Class="UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UI"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="BorderColor" Color="#51504D"/>
            <SolidColorBrush x:Key="LightTile" Color="#EFEFEF"/>
            <SolidColorBrush x:Key="DarkTile" Color="#C2D7E2"/>
            <SolidColorBrush x:Key="PrevMove" Color="LightCoral"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
